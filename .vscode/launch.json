{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "port": 5858,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch via Yarn",
      "request": "launch",
      "runtimeArgs": [
        "start",
        "publish"
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ],
}
